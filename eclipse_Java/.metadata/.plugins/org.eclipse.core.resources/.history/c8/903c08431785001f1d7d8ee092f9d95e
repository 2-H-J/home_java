	public static void main(String[] args) {
		try (Socket client = new Socket("127.0.0.1",3030);){
			PrintWriter pw = new PrintWriter(client.getOutputStream());
			
		} catch (IOException e) {
			e.printStackTrace();
		}
	}

}
